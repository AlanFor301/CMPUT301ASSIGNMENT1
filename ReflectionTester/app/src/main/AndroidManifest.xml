        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LaunchPage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NumberChooseActivity"
            android:label="@string/title_activity_number_choose" >
        </activity>
        <activity
            android:name=".SinglePlayerReadyCheck"
            android:label="@string/title_activity_single_player_ready_check" >
        </activity>
    </application>

</manifest>
